{
	"name": "@acme/feature-alpha",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		".": "./src/index.ts",
		"./App.svelte": "./src/App.svelte"
	},
	"scripts": {
		"format": "pnpm -w exec prettier --write \"$PNPM_SCRIPT_SRC_DIR\"",
